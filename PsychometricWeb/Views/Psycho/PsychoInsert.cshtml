                        isblank = true;
                        return false; // Exit the loop
                    }
                    else if (typeof leastInput === 'undefined') {
                        isValid = false;
                        // Highlight the row with missing input
                        $(this).css("background-color", "#ffcccc");
                        $(this).find('input[type="radio"]:not(:checked):enabled').first().focus();
                        isblank = true;
                        return false; // Exit the loop
                    }
                    else {
                        // Remove any previous highlighting
                        $(this).css("background-color", "");
                    }
                    if (mostInput !== "" && leastInput !== "") {
                        formData.A.push($(row).find('.a-input').text());
                        formData.B.push($(row).find('.b-input').text());
